universe=vanilla
# note: The executable must exist on the submit node
executable=my_example.sh
transfer_executable=True
when_to_transfer_output = ON_EXIT
log=example.log
output=exaple.out.$(Cluster).$(Process)
error=example.err.$(Cluster).$(Process)
